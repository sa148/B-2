<% if @book.errors.any? %>
    <%= @book.errors.count %>
      <ul>
        <% @book.errors.full_messages.each do |message|%>
        <li><%= message%></li>
        <% end %>
      </ul>
  <% end %>
  <%= form_with mldel: @book do |f| %>
    <h4>title</h4>
    <%= f.text_field :title%>
    <h4>body</h4>
    <%= f.text_area :body%>
    <br><%= f.submit "Create Book"%>
  <% end %>